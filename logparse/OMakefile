# Use the byte-code compiler
BYTE_ENABLED = true
NATIVE_ENABLED = false
OCAMLCFLAGS += -g -thread -annot

USE_OCAMLFIND = true
OCAMLPACKS[] = extlib batteries

FILES = parselog

.PHONY: clean distclean run

clean:
	rm -f *.cmo *.cmi parselog *.run

run: parselog
	./parselog -d -v -f ../refine.txt.out

run1: parselog
	./parselog -d -v -f ../refine_fail.txt.out


#Build the program
OCamlProgram(parselog, $(FILES))
.DEFAULT: parselog

