# Use the byte-code compiler
BYTE_ENABLED = true
NATIVE_ENABLED = false
OCAMLCFLAGS += -g -thread

USE_OCAMLFIND = true
OCAMLPACKS[] = extlib batteries

FILES = parselog

.PHONY: clean distclean run

clean:
	rm -f *.cmo *.cmi parselog *.run

run: parselog
	./parselog -d -v -f ../refine_log.txt.out

#Build the program
OCamlProgram(parselog, $(FILES))
.DEFAULT: parselog

